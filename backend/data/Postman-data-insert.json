    "INSERTION_ORDER": [
        "1. Insert all CLASSROOMS (6 items)",
        "2. Insert all BATCHES (6 items)",
        "3. Go to MongoDB and copy the _id of each batch created",
        "4. Insert SUBJECTS for each batch (replace <BATCH_ID> placeholders with actual ObjectIds)",
        "5. Go to MongoDB and copy the _id of each subject created",
        "6. Insert FACULTY (replace <SUBJECT_ID> placeholders in qualifiedSubjects arrays)",
        "7. Insert STUDENTS (replace <BATCH_ID> placeholders)"
    ],
    "POSTMAN_TIPS": [
        "Save your admin JWT token as an environment variable in Postman",
        "Use {{admin_token}} in Authorization header",
        "After inserting batches, query them: GET http://localhost:5000/api/admin/batches",
        "Copy _id values from responses to use in subsequent requests",
        "For arrays like qualifiedSubjects, use actual ObjectId strings like: ['6756abc123def456789', '6756abc987fed654321']"
    ],
    "MAPPING_REFERENCE": {
        "mock_to_mongodb": {
            "rooms.type": {
                "lecture_hall": "Classroom",
                "computer_lab": "Laboratory"
            },
            "courses.requires_lab": {
                "true": "type: 'Practical', requiredRoomType: 'Laboratory'",
                "false": "type: 'Theory', requiredRoomType: 'Classroom'"
            },
            "teachers.unavailable_slots": "unavailableTimeSlots: [[day, slot], ...]",
            "teachers.qualified_courses": "qualifiedSubjects: [ObjectId, ObjectId, ...]"
        }
    }
